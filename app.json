{
	"expo": {
		"name": "Pok√©mon Card Collector",
		"slug": "pokemon-card-collector",
		"version": "1.0.0",
		"android": {
			"package": "com.nodon.pokemoncardcollector"
		},
		"ios": {
			"bundleIdentifier": "com.nodon.pokemoncardcollector"
		},
		"newArchEnabled": true,
		"plugins": [
			[
				"react-native-edge-to-edge",
				{
					"android": {
						"parentTheme": "Material3.Dynamic.Light"
